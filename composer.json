    "require-dev": {
        "phpunit/phpunit": "^8.1",
        "squizlabs/php_codesniffer": "^3.4",
        "phpcompatibility/php-compatibility": "^9.1",
        "sensiolabs/security-checker": "^5.0 || ^6.0",
        "phpmd/phpmd": "^2.6"
    },
    "autoload": {
        "psr-4": {
